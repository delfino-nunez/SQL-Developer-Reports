	<name><![CDATA[SQL Statements Execution Time (AWR)]]></name>
	<description><![CDATA[]]></description>
	<tooltip><![CDATA[]]></tooltip>
	<drillclass><![CDATA[]]></drillclass>
	<CustomValues>
		<TYPE><![CDATA[horizontal]]></TYPE>
	</CustomValues>
	<query>
		<sql><![CDATA[SELECT "SECS_PER_EXEC", "EXECUTIONS", "SQL_ID", "SQL_TEXT" FROM(
with stats as (select round(sum(elapsed_time_delta)/1000000/decode(sum(executions_delta),0,1,      
                      sum(executions_delta)),2) secs_per_exec,      
                      sum(executions_delta) executions, sql_id      
               from dba_hist_sqlstat group by sql_id)      
select secs_per_exec, executions, stats.sql_id, sql_text      
from stats, dba_hist_sqltext st      
where stats.sql_id = st.sql_id      
order by 1 desc
)]]></sql>
	</query>
