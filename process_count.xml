	</CustomValues>
	<query>
		<sql><![CDATA[SELECT "INST_ID", "USERNAME", "PROGRAM", "MACHINE", "COUNT(*)" FROM(
SELECT s.inst_id,
       s.username,
       s.program,
       machine,
       count(*)
FROM   gv$session s
       JOIN gv$process p ON p.addr = s.paddr AND p.inst_id = s.inst_id
WHERE  s.type != 'BACKGROUND'
group by s.inst_id,
       s.username,
       s.program,
       machine
order by inst_id
)]]></sql>
	</query>
