	<query>
		<sql><![CDATA[SELECT "SID", "SERIAL#", "MACHINE", "USERNAME", "ELAPSED", "REMAINING", "PROGRESS_PCT", "TARGET", "OPNAME", "MESSAGE", "SQL_EXEC_START" FROM(
SELECT s.sid,
       s.serial#,
       s.machine,
       s.username,
       ROUND(sl.elapsed_seconds/60) || ':' || MOD(sl.elapsed_seconds,60) elapsed,
       ROUND(sl.time_remaining/60) || ':' || MOD(sl.time_remaining,60) remaining,
       ROUND(sl.sofar/decode(sl.totalwork,0,1)*100, 2) progress_pct,
       sl.target,
       sl.opname,
       sl.message,
       sl.sql_exec_start
FROM   gv$session s,
       gv$session_longops sl
WHERE  s.sid     = sl.sid
AND    s.serial# = sl.serial#
)]]></sql>
	</query>
