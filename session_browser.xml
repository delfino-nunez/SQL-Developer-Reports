	<name><![CDATA[Session Browser]]></name>
	<description><![CDATA[Show sessions connected to the database		]]></description>
	<tooltip><![CDATA[]]></tooltip>
	<drillclass><![CDATA[]]></drillclass>
	<CustomValues>
		<TYPE><![CDATA[horizontal]]></TYPE>
	</CustomValues>
	<query>
		<sql><![CDATA[SELECT 
  s.inst_id,
  s.SID,
  s.SERIAL#,
  p.spid,
  p.pid,
  s.PROCESS,
  s.OSUSER,
  s.USERNAME,
  s.STATUS,
  s.SERVER,
  s.MACHINE,
  s.TERMINAL,
  s.LOGON_TIME,
  s.SCHEMANAME,
  s.RESOURCE_CONSUMER_GROUP,
  p.program "OSProgram",
  UPPER(s.PROGRAM) PROGRAM,
  s.TYPE,
  s.SQL_ADDRESS,
  s.SQL_HASH_VALUE,
  s.PREV_SQL_ADDR,
  s.PREV_HASH_VALUE,
  s.MODULE,
  s.CLIENT_INFO,
  s.ROW_WAIT_OBJ#,
  s.ROW_WAIT_FILE#,
  s.ROW_WAIT_BLOCK#,
  s.ROW_WAIT_ROW#,
  s.paddr
FROM GV$SESSION S,
  GV$PROCESS p
WHERE ( (s.USERNAME    IS NOT NULL)
AND (NVL(s.osuser,'x') <> 'SYSTEM')
AND (s.type            <> 'BACKGROUND') )
AND (p.addr(+)          =s.paddr)]]></sql>
	</query>
