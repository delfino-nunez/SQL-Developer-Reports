	<query>
		<sql><![CDATA[SELECT "INST_ID", "GROUP#", "THREAD#", "MB", "STATUS", "TYPE", "MEMBER" FROM(
select l.inst_id,l.group#,l.thread#, l.bytes / 1024/1024 Mb, l.status, f.type,f.member
from gv$log l inner join gv$logfile f on (l.inst_id = f.inst_id and l.group# = f.group#)
order by inst_id,group#,thread#
)]]></sql>
	</query>
