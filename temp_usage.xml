	<query>
		<sql><![CDATA[SELECT "SID", "OSUSER", "PROCESS", "USERNAME", "SQL_ID", "SEGTYPE", "MB", "TABLESPACE" FROM(
select  s.sid, s.osuser, s.process, s.username,
        s.sql_id, tmp.segtype, 
       ((tmp.blocks*8)/1024)MB, tmp.tablespace
from  
       v$tempseg_usage tmp,  
       v$session s
where tmp.session_num=s.serial#
and segtype in ('HASH','SORT')
order by blocks desc
)]]></sql>
	</query>
